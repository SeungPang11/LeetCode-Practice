# Jump Game - Dynamic Programming
nums1 = [2,3,1,1,4] #True
nums2 = [3,2,1,0,4] #False

def jump(num):
    n = len(num)
    dp = [False for _ in range (n)]
    dp[0] = True

    for i in range(n):
        if dp[i] == True:
            for j in range(1, num[i]+1):
                if i+j < n:
                    dp[i+j] = True
                if i+j == n-1:
                    return True
    return dp[n-1]

res = jump(nums2)
print(res)
