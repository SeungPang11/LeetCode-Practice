class Node:
    def __init__(self, val):
        self.val = val
        self.next = None

class Queue:
    def __init__(self):
        self.head = None

    def isEmpty(self):
        return True if self.head == None else False

    def enqueue(self, val):
        if self.head == None:
            self.head = Node(val)
        else:
            # Insert at the end of list
            temp = self.head
            while temp.next != None:
                temp = temp.next
            temp.next = Node(val)

    # Disconnect front of the list
    def dequeue(self):
        temp = self.head
        self.head = temp.next

    def display(self):
        temp = self.head
        while temp != None:
            print(temp.val)
            temp = temp.next

if __name__ == "__main__":
    queue = Queue()
    queue.enqueue(1)
    queue.enqueue(2)
    queue.enqueue(3)
    queue.display()
    
    print("After dequeue")
    queue.dequeue()
    queue.dequeue()
    queue.display()
